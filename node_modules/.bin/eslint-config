#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/novikovapolina/WebstormProjects/chat-bot-app/node_modules/.pnpm/@antfu+eslint-config@2.1.2_eslint@8.54.0_typescript@5.3.2/node_modules/@antfu/eslint-config/bin/node_modules:/Users/novikovapolina/WebstormProjects/chat-bot-app/node_modules/.pnpm/@antfu+eslint-config@2.1.2_eslint@8.54.0_typescript@5.3.2/node_modules/@antfu/eslint-config/node_modules:/Users/novikovapolina/WebstormProjects/chat-bot-app/node_modules/.pnpm/@antfu+eslint-config@2.1.2_eslint@8.54.0_typescript@5.3.2/node_modules/@antfu/node_modules:/Users/novikovapolina/WebstormProjects/chat-bot-app/node_modules/.pnpm/@antfu+eslint-config@2.1.2_eslint@8.54.0_typescript@5.3.2/node_modules:/Users/novikovapolina/WebstormProjects/chat-bot-app/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/novikovapolina/WebstormProjects/chat-bot-app/node_modules/.pnpm/@antfu+eslint-config@2.1.2_eslint@8.54.0_typescript@5.3.2/node_modules/@antfu/eslint-config/bin/node_modules:/Users/novikovapolina/WebstormProjects/chat-bot-app/node_modules/.pnpm/@antfu+eslint-config@2.1.2_eslint@8.54.0_typescript@5.3.2/node_modules/@antfu/eslint-config/node_modules:/Users/novikovapolina/WebstormProjects/chat-bot-app/node_modules/.pnpm/@antfu+eslint-config@2.1.2_eslint@8.54.0_typescript@5.3.2/node_modules/@antfu/node_modules:/Users/novikovapolina/WebstormProjects/chat-bot-app/node_modules/.pnpm/@antfu+eslint-config@2.1.2_eslint@8.54.0_typescript@5.3.2/node_modules:/Users/novikovapolina/WebstormProjects/chat-bot-app/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@antfu/eslint-config/bin/index.js" "$@"
else
  exec node  "$basedir/../@antfu/eslint-config/bin/index.js" "$@"
fi
